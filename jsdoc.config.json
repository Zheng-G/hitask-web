{
	"opts": {
		"recurse": true,
		"destination": "./docs/"
	},
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"include": [
			"packages/modules",
			"packages/utils",
			"packages/constants"
		],
		"exclude": ["node_modules/"],
		"excludePattern": "node_modules"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
    },
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}
